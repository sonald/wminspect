{
  "permissions": {
    "allow": [
      "Bash(cargo search:*)",
      "Bash(cargo:*)",
      "Bash(brew list:*)",
      "Bash(brew install:*)",
      "Bash(pkg-config:*)",
      "Bash(echo $LIBRARY_PATH)",
      "Bash(echo $PKG_CONFIG_PATH)",
      "Bash(rustc:*)",
      "Bash(git add:*)",
      "Bash(git rm:*)",
